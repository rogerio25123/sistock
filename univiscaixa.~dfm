  object sqltotalgrupo: TZQuery
    Connection = dm2.conexao
    SQL.Strings = (
      
        'SELECT  sum(total)  as totalgrupo,formapgto, vendedor from dados' +
        'vendas  group by vendedor, formapgto')
    Params = <>
    Left = 464
    Top = 67
    object sqltotalgrupototalgrupo: TFloatField
      FieldName = 'totalgrupo'
      ReadOnly = True
      DisplayFormat = '0.00'
    end
    object sqltotalgrupoformapgto: TStringField
      FieldName = 'formapgto'
      Size = 30
    end
    object sqltotalgrupovendedor: TStringField
      FieldName = 'vendedor'
      Size = 40
    end
  end
