  object srcVPC: TDataSource
    DataSet = sqlVPC
    Left = 589
    Top = 128
  end
  object sqlVPC: TZQuery
    Connection = dm2.conexao
    OnCalcFields = sqlVPCCalcFields
    Active = True
    SQL.Strings = (
      
        'select dv.*,c.cliente, iv.*, count(iv.codprod) qtd from dadosven' +
        'das dv, itensvendas iv, clientes c'
      'where dv.codvenda=iv.codvenda  '
      'and dv.codcli = c.codcli'
      'and dv.data >= '#39'2018-08-26'#39
      'and dv.data <= '#39'2018-08-26'#39
      'group by dv.codvenda,iv.codprod, dv.data  order by iv.descricao')
    Params = <>
    Left = 557
    Top = 128
    object sqlVPCcodvenda: TIntegerField
      FieldName = 'codvenda'
      Required = True
