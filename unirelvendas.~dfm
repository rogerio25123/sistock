  object sqlPV: TZQuery
    Connection = dm2.conexao
    Active = True
    SQL.Strings = (
      
        'select  dv.data, iv.*, count(iv.codprod) qtd from dadosvendas dv' +
        ', itensvendas iv'
      'where dv.codvenda=iv.codvenda  '
      'and dv.data >= '#39'2018-08-26'#39
      'and dv.data <= '#39'2018-08-26'#39
      'group by iv.codprod, dv.data order by iv.descricao')
    Params = <>
    Left = 357
    Top = 128
    object sqlPVdata: TDateField
      FieldName = 'data'
    end
    object sqlPVcodprod: TIntegerField
      FieldName = 'codprod'
    end
    object sqlPVdescricao: TStringField
      FieldName = 'descricao'
      Size = 120
    end
    object sqlPVunivenda: TStringField
      FieldName = 'univenda'
      Size = 12
    end
    object sqlPVqtd: TFloatField
      FieldName = 'qtd'
    end
    object sqlPVpreco: TFloatField
      FieldName = 'preco'
      DisplayFormat = '###,###,##0.00'
    end
  end
  object src: TDataSource
    DataSet = sqlPV
    Left = 397
    Top = 128
  end
  object sqlPVC: TZQuery
    Connection = dm2.conexao
    SQL.Strings = (
      
        'select dv.codvenda,  dv.data,dv.codcli,c.cliente, iv.*, count(iv' +
        '.codprod) qtd from dadosvendas dv, itensvendas iv, clientes c'
      'where dv.codvenda=iv.codvenda  '
      'and dv.codcli = c.codcli'
      'and dv.data >= '#39'2018-08-26'#39
      'and dv.data <= '#39'2018-08-26'#39
      'group by iv.codprod, dv.data order by iv.descricao')
    Params = <>
    Left = 469
    Top = 128
    object DateField1: TDateField
      FieldName = 'data'
    end
    object IntegerField1: TIntegerField
      FieldName = 'codprod'
    end
    object StringField1: TStringField
      FieldName = 'descricao'
      Size = 120
    end
    object StringField2: TStringField
      FieldName = 'univenda'
      Size = 12
    end
    object FloatField1: TFloatField
      FieldName = 'qtd'
    end
    object FloatField2: TFloatField
      FieldName = 'preco'
      DisplayFormat = '###,###,##0.00'
    end
    object sqlPVCcodvenda: TIntegerField
      FieldName = 'codvenda'
      Required = True
    end
    object sqlPVCcodcli: TIntegerField
      FieldName = 'codcli'
    end
    object sqlPVCcliente: TStringField
      FieldName = 'cliente'
      Size = 55
    end
  end
