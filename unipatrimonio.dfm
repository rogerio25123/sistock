  object sqlsaidasprodutos: TZQuery
    Connection = dm2.conexao
    SQL.Strings = (
      
        'SELECT itensvendas.descricao, SUM( itensvendas.qtd ) totalsaida,' +
        ' Produtos.Estoque, Produtos.EstoqueMinimo, itensvendas.Codprod'
      'FROM itensvendas'
      '   INNER JOIN dadosvendas'
      '   ON  (dadosvendas.codvenda = itensvendas.codvenda)  '
      '   INNER JOIN   Produtos'
      '   ON  (itensvendas.Codprod = Produtos.CodProd)  '
      
        'GROUP BY itensvendas.descricao, Produtos.Estoque, Produtos.Estoq' +
        'ueMinimo, itensvendas.Codprod'
      'ORDER BY totalsaida DESC')
    Params = <>
    Left = 256
    Top = 64
    object sqlsaidasprodutosdescricao: TStringField
      FieldName = 'descricao'
      Size = 120
    end
    object sqlsaidasprodutostotalsaida: TFloatField
      FieldName = 'totalsaida'
      ReadOnly = True
    end
    object sqlsaidasprodutosEstoque: TFloatField
      FieldName = 'Estoque'
    end
    object sqlsaidasprodutosEstoqueMinimo: TFloatField
      FieldName = 'EstoqueMinimo'
    end
